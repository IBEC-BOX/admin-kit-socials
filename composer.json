{
    "name": "ibecsystems/admin-kit-social-medias",
    "description": "SocialMedias package for AdminKit",
    "type": "admin-kit-package",
    "keywords": [
        "ibec-box",
        "laravel",
        "admin-kit-social-medias"
    ],
    "homepage": "https://github.com/ibec-box/admin-kit-social-medias",
    "license": "MIT",
    "authors": [
        {
            "name": "daurensky",
            "email": "dkambarov17@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0-stable",
        "ibecsystems/admin-kit-core": "^3.1",
        "illuminate/contracts": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.14.0",
        "spatie/laravel-data": "^4.5",
        "spatie/laravel-json-api-paginate": "^1.13",
        "spatie/laravel-query-builder": "^5.7",
        "spatie/laravel-translatable": "^6.5"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9|^8.1",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AdminKit\\SocialMedias\\": "src/",
            "AdminKit\\SocialMedias\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdminKit\\SocialMedias\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdminKit\\SocialMedias\\SocialMediasServiceProvider"
            ],
            "aliases": {
                "SocialMedias": "AdminKit\\SocialMedias\\Facades\\SocialMedias"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
